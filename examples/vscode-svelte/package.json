{
	"private": true,
	"name": "vscode-svelte",
	"version": "1.0.22",
	"repository": {
		"type": "git",
		"url": "https://github.com/johnsoncodehk/volar.git",
		"directory": "examples/vscode-svelte"
	},
	"displayName": "Svelte Language Server (Example)",
	"description": "Svelte Language Server Example",
	"author": "johnsoncodehk",
	"publisher": "johnsoncodehk",
	"engines": {
		"vscode": "^1.67.0"
	},
	"activationEvents": [
		"onLanguage:svelte"
	],
	"main": "out/client",
	"contributes": {
		"commands": [
			{
				"command": "volar.action.showVirtualFiles",
				"title": "Show Virtual Files",
				"category": "Volar (Debug)"
			}
		],
		"languages": [
			{
				"id": "svelte",
				"extensions": [
					".svelte"
				]
			}
		]
	},
	"scripts": {
		"release": "vsce publish"
	},
	"devDependencies": {
		"@types/vscode": "1.67.0",
		"vsce": "latest"
	},
	"dependencies": {
		"@volar-examples/svelte-language-server": "1.0.22",
		"@volar/vscode-language-client": "1.0.22",
		"typesafe-path": "^0.2.2",
		"vscode-languageclient": "^8.0.2"
	}
}
