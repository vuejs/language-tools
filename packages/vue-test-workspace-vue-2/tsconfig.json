{
    "compilerOptions": {
        "lib": [
            "esnext",
            "dom"
        ],
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "skipLibCheck": true,
        "allowJs": true,
        "jsx": "preserve",
    },
    "include": [
        "**/*"
    ]
}
